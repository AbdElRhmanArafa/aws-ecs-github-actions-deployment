{
    "family": "myapp-task-def-family",
    "networkMode": "awsvpc",
    "containerDefinitions": [{
        "name": "myapp",
        "image": "283487706052.dkr.ecr.us-east-1.amazonaws.com/myapp:latest",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80
        }],
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/myapp-log-group",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::283487706052:role/LabRole",
    "taskRoleArn": "arn:aws:iam::283487706052:role/LabRole"
}